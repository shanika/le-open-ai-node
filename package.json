{
  "name": "your-project-name",
  "version": "1.0.0",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c && tsc node.config.ts --outDir dist --module esnext --target es2018 --moduleResolution node --declaration false && node scripts/generateConfig.js && npm run generate-types",
