    "generate-types": "NODE_OPTIONS=\"--loader ts-node/esm\" node scripts/generateTypes.ts"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.17.6",
    "rollup": "^4.12.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
